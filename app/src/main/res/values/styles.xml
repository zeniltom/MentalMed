<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:statusBarColor">?android:attr/colorForeground</item>
    </style>

    <style name="dialogEmpregosAL" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="DialogTitleAppearance">@android:style/TextAppearance.Large</item>
        <item name="DialogSpotColor">@color/colorPrimaryDark</item>
        <item name="DialogSpotCount">8</item>
    </style>

</resources>
